{
  "private": true,
  "name": "example-preact",
  "version": "2.0.10",
  "description": "Fela example with Preact",
  "scripts": {
    "client": "NODE_ENV=development webpack-dev-server",
    "start": "concurrently --kill-others \"npm run client\" \"babel-node server.js\""
  },
  "author": "Robin Frischmann <robin@rofrischmann.de>",
  "license": "MIT",
  "dependencies": {
    "fela": "^6.0.0",
    "fela-beautifier": "^5.0.7",
    "fela-dom": "^7.0.0",
    "fela-perf": "^5.0.2",
    "fela-plugin-embedded": "^5.1.2",
    "fela-plugin-fallback-value": "^5.0.8",
    "fela-plugin-logger": "^5.0.3",
    "fela-plugin-lvha": "^5.0.7",
    "fela-plugin-prefixer": "^5.0.8",
    "fela-plugin-unit": "^5.0.7",
    "fela-plugin-validator": "^5.0.7",
    "preact": "^8.1.0",
    "preact-fela": "^6.1.1",
    "preact-render-to-string": "^3.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^3.1.0",
    "express": "^4.14.0",
    "express-http-proxy": "^1.0.3",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
